root ::= Recipe
Ingredient ::= "{"   ws   "\"name\":"   ws   string   ","   ws   "\"quantity\":"   ws   string   "}"
Ingredientlist ::= "[]" | "["   ws   Ingredient   (","   ws   Ingredient)*   "]"
Recipe ::= "{"   ws   "\"ingredients\":"   ws   Ingredientlist   ","   ws   "\"instructions\":"   ws   string   "}"
Recipelist ::= "[]" | "["   ws   Recipe   (","   ws   Recipe)*   "]"
string ::= "\""   ([^"]*)   "\""
boolean ::= "true" | "false"
ws ::= [ \t\n]*
number ::= [0-9]+   "."?   [0-9]*
stringlist ::= "["   ws   "]" | "["   ws   string   (","   ws   string)*   ws   "]"
numberlist ::= "["   ws   "]" | "["   ws   string   (","   ws   number)*   ws   "]"
